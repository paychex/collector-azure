{
    "name": "@paychex/collector-azure",
    "version": "2.0.1",
    "description": "Provides an Azure Event Bus collector for use with a @paychex/core Tracker.",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "files": [
        "dist",
        "docs"
    ],
    "scripts": {
        "test": "mocha --recursive",
        "coverage": "c8 --reporter=html mocha --recursive",
        "docs": "jsdoc -c jsdoc.json",
        "build": "rollup -c",
        "clean": "rimraf dist docs coverage",
        "preversion": "npm run clean && npm run docs && npm run build",
        "prepublish": "c8 --reporter=html --check-coverage --branches 100 --functions 100 --lines 100 --statements 100 mocha --recursive"
    },
    "keywords": [
        "paychex",
        "azure",
        "event hub",
        "collector",
        "tracking"
    ],
    "license": "Apache-2.0",
    "author": "Paychex, Inc.",
    "contributors": [
        {
            "name": "Dan Barnes",
            "email": "dbarnes1@paychex.com"
        }
    ],
    "devDependencies": {
        "@paychex/core": "^2.0.0",
        "@rollup/plugin-commonjs": "^17.1.0",
        "@rollup/plugin-node-resolve": "^11.2.0",
        "c8": "^7.3.5",
        "docdash": "^1.2.0",
        "expect": "^26.0.1",
        "jsdoc": "^3.6.4",
        "mocha": "^8.3.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.39.1"
    },
    "dependencies": {
        "@azure/event-hubs": "^5.3.1"
    },
    "peerDependencies": {
        "lodash-es": "^4.17.21"
    }
}
